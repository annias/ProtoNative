{
  "name": "protonative",
  "version": "1.0.0",
  "description": "A React Native prototyping tool",
  "main": "./src/electron.ts",
  "scripts": {
    "start": "concurrently \"npm run vite\" \"node -e \\\"setTimeout(() => process.exit(0), 1000)\\\" && npm run electron\"",
    "vite": "vite",
    "electron": "cross-env NODE_ENV=development electron .",
    "build:windows": "npm run build && electron-builder --windows",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --verbose"
  },
  "author": "Team ProtoNative",
  "contributors": [
    "Matthew Kymn",
    "Michelle Leong",
    "Jonathan Klibansky",
    "Patricia Good",
    "Raymond Ferrer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/ProtoNative.git"
  },
  "keywords": [
    "electron",
    "prototyping",
    "react",
    "native"
  ],
  "homepage": "https://github.com/oslabs-beta/ProtoNative#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/dagre": "^0.7.48",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.5.0",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-jest": "^29.5.0",
    "concurrently": "8.0.1",
    "cross-env": "^7.0.3",
    "dagre": "^0.8.5",
    "electron": "^23.2.0",
    "electron-builder": "^24.1.1",
    "electron-reloader": "1.2.3",
    "eslint": "^8.37.0",
    "file-saver": "^2.0.5",
    "jest": "^29.5.0",
    "jszip": "^3.10.1",
    "prettier": "2.8.7",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "reactflow": "^11.7.0",
    "sass": "^1.60.0",
    "scss": "^0.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  },
  "build": {
    "appId": "protonative",
    "productName": "ProtoNative",
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "./build/icons/windowsIcon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "./build/icons/macIcon.icns"
    },
    "linux": {
      "target": "AppImage"
    },
    "files": [
      "build/**/*",
      "src/electron.ts"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
